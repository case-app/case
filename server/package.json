{
  "name": "case-server",
  "version": "0.0.0",
  "description": "description",
  "author": "Buddyweb",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "node dist/main.js",
    "start:hmr": "node dist/server",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js",
    "seed": "ts-node database/seeders/seeder.ts && cp -R assets/dummy/* public/storage/",
    "case:resourceBack": "schematics @case-app/schematics/collection.json:createResourceServer && npm run seed"
  },
  "dependencies": {
    "@bugsnag/js": "^7.9.0",
    "@bugsnag/plugin-express": "^7.9.0",
    "@case-app/nest-library": "^0.0.4",
    "@nestjs/common": "^6.5.3",
    "@nestjs/core": "^6.5.3",
    "@nestjs/platform-express": "^6.5.3",
    "@nestjs/typeorm": "^6.1.3",
    "@nkaurelien/nest-bugsnag": "^6.1.0",
    "bluebird": "^3.7.2",
    "class-transformer": ">=0.3.1",
    "class-validator": "^0.9.1",
    "compression": "^1.7.4",
    "crypto-js": "^3.1.9-1",
    "docxtemplater": "^3.19.0",
    "exceljs": "^1.13.0",
    "express": "^4.17.1",
    "handlebars": "^4.7.6",
    "http-status-codes": "^1.4.0",
    "i": "^0.3.7",
    "jsonwebtoken": "^8.5.1",
    "mailgun-js": "^0.22.0",
    "moment-business-days": "^1.2.0",
    "mysql": "^2.17.1",
    "nest-schedule": "^0.6.3",
    "npm": "^6.14.8",
    "pizzip": "^3.0.6",
    "puppeteer": "^1.20.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.2",
    "sharp": "^0.27.0",
    "slugify": "^1.3.5",
    "typeorm": "^0.2.18",
    "typescript": "^3.0.1",
    "uniqid": "^5.0.3"
  },
  "devDependencies": {
    "@nestjs/testing": "6.1.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.0",
    "@types/faker": "^5.5.8",
    "@types/jest": "^23.3.1",
    "@types/mailgun-js": "^0.22.3",
    "@types/node": "^10.17.59",
    "@types/supertest": "^2.0.5",
    "faker": "^5.5.3",
    "jest": "^25.0.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
